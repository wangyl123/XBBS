
<div class='box'>
    <div class='box-header'>
        最新标签
    </div>
    <div class='inner'>
        @{
            if (ViewBag.TagList != null)
            {
                <ul class="list_unstyled">
                    @{
                foreach (var item in ViewBag.TagList)
                {
                    <span class=" tags">
                        <a href="/tag/index/@item.TagTitle">@item.TagTitle</a>
                    </span>
                }
                    }
                </ul>

            }

        }
    </div>
</div>